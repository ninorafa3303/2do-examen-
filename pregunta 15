def busqueda(lista, x):
    izq = 0
    der = len(lista) - 1 

    while izq <= der:
        medio = (izq + der) // 2

        print("DEBUG:", "izq:", izq, "der:", der, "medio:", medio)
        if lista[medio] == x:
            return medio

        elif lista[medio] > x:
            der = medio - 1

        else: 
            izq = medio + 1

    return -1  # Retornar -1 si el elemento no se encuentra
