def ordenamiento(lista):
    for i in range(1, len(lista)):  
        print('DEBUG: FOR del i= ', i)
        valor_actual = lista[i]  
        j = i - 1 

        while j >= 0 and lista[j] > valor_actual:
            lista[j + 1] = lista[j]
            print('DEBUG: ', lista)
            j -= 1
        
        lista[j + 1] = valor_actual  
        print('DEBUG: Inserción de valor_actual en posición correcta', lista)


        
        print('--------- FIN I, PROXIMO I ---------')
